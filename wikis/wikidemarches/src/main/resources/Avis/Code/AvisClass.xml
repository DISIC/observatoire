{{html wiki="false"}}
#set ($className = ${object.xWikiClass.name})
#if ($type == 'edit')
  #foreach ($val in [0, 1])
  #set ($checked = ($value == $val))
  #if ($checked)#set ($checked = "checked")#else#set ($checked = "")#end
  &lt;div&gt;
    &lt;label for="${prefix}${name}_${val}"&gt;&lt;input id="${prefix}${name}_${val}" name="${prefix}${name}" type="radio" value="$val" $checked&gt; 
    $services.localization.render("${className}_${name}_$val")
    &lt;/label&gt;
  &lt;/div&gt;
  #end
  &lt;label for="${prefix}${name}_none"&gt;
    &lt;input type="hidden" id="${prefix}${name}_none" name="${prefix}${name}" value=""&gt; 
  &lt;/label&gt;
#else
$value
#end
{{/html}}
{{/velocity}}

