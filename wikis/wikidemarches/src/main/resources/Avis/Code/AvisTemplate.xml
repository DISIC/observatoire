  #end
  &lt;label for="${prefix}${name}_none"&gt;
    &lt;input type="hidden" id="${prefix}${name}_none" name="${prefix}${name}" value=""&gt; 
  &lt;/label&gt;
#else
$value
#end
{{/html}}
