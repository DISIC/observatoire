    <property>
      <content>#if ($type == 'edit' || $type == 'search')
  #if ($field.getProperty('displayType').value == 'input' &amp;&amp; $field.getProperty('picker').value == 1)
    #set ($id = $escapetool.xml("${prefix}${name}"))
    #set ($disabled = $field.getProperty('disabled').value == 1)
