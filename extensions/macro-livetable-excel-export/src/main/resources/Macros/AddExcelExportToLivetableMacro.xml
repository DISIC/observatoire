{{html clean="false"}}

&lt;script language="javascript"&gt;

/**
* Function that add the excel export button to the livetable
*/
function addExcelButton(){
  var livetableName = "livetable_$escapetool.javascript($xcontext.macro.params.livetable)";
  var livetable = window[livetableName];
  livetable.exportToExcel = XWiki.widgets.LiveTable.exportToExcel;
  // add the button after the livetable
  var livetableElt = $('$escapetool.javascript($xcontext.macro.params.livetable)');
  if (!livetableElt) {
    return;
  }
  // add a button with the text and the icon
  #set ($format = "$!xcontext.macro.params.format")
  #set ($btnLabel = "$!xcontext.macro.params.label")
  #if ($btnLabel == '')
    #set ($prettyFormat = $services.localization.render("xwiki.macro.exportexcellivetable.format.$format"))
    #set ($btnLabel = $services.localization.render('xwiki.macro.exportexcellivetable.title', $prettyFormat))
    #set ($btnIconTitle = $services.localization.render('xwiki.macro.exportexcellivetable.icontitle', $prettyFormat))
  #else
    #set ($btnIconTitle = $btnLabel)
  #end
  var excel_span_title = new Element('span', {'class': 'xwiki-livetable-tools-text'}).update('$btnLabel');
  var excel_span_icon = new Element('span', {'class': 'xwiki-livetable-tools-excel'});
  var excel_button = new Element('button', {'class' : 'button', 'title':"$escapetool.javascript($btnIconTitle)"});
  excel_button.appendChild(excel_span_title);
  #if ($format == 'excel')
    excel_button.appendChild(excel_span_icon);
  #end
  excel_button.observe('click', function() { livetable.exportToExcel($xcontext.macro.params.withPageName, $xcontext.macro.params.withPageURL, $xcontext.macro.params.withAllFields, "$escapetool.javascript("$!xcontext.macro.params.filename")", "$escapetool.javascript($format)", "$escapetool.javascript("$!xcontext.macro.params.customResults")"); });
  var excel_span = new Element('span', {'class':'buttonwrapper xwiki-livetable-tools-excelexport'});
  excel_span.appendChild(excel_button);
  // add the excel span after table
  livetableElt.insert({'after' : excel_span});
}

document.observe("xwiki:dom:loaded", addExcelButton);

&lt;/script&gt;

{{/html}}
